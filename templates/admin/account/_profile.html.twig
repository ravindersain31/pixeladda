<table class="table table-bordered">
    <tbody>
    <tr>
        <th>Name</th>
        <td>{{ user.name }}</td>
    </tr>
    <tr>
        <th>Username</th>
        <td>{{ user.username }}</td>
    </tr>
    <tr>
        <th>EMail</th>
        <td>{{ user.email }}</td>
    </tr>
    <tr>
        <th>Phone</th>
        <td>{{ user.mobile }}</td>
    </tr>
    <tr>
        <th>Last Login At</th>
        <td>{{ user.lastLoginAt | date('M d, Y') }}</td>
    </tr>
    </tbody>
</table>