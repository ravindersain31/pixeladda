<div {{ attributes }}>
    {{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="row">
      <div class="col-12">
        {{ form_row(form.proofFile) }}
      </div>
      <div class="col-6">
        {{ form_row(form.refNumber) }}
      </div>
      <div class="col-6">
        {{ form_row(form.poNumber) }}
      </div>
      <div class="col-12">
        {{ form_row(form.comment) }}
      </div>
      <div class="col-12">
        {{ form_row(form.currency) }}
      </div>
      <div class="col-12">
        {{ form_row(form.submit) }}
      </div>
    </div>
  {{ form_end(form) }}
</div>