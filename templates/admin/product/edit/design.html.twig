<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Yard Sign Plus{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <script data-search-pseudo-elements defer
            src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/js/all.min.js"
            crossorigin="anonymous"></script>
    {% block stylesheets %}
        {{ encore_entry_link_tags('admin-editor') }}
        {{ encore_entry_link_tags('admin') }}
    {% endblock %}
</head>
<body>
<div id="product-editor-container" {{ react_component('AdminProductEditor', {
    'variant': variant,
    'saveUrl': saveUrl,
    'uploadDataImage': uploadDataImage,
    'templateJsonUrl': templateJsonUrl
}) }}>
    <div class="loading-product-editor">
        Initializing Product Editor...
    </div>
</div>
{% block javascripts %}
    {{ encore_entry_script_tags('admin-editor') }}
    {{ encore_entry_script_tags('admin') }}
{% endblock %}
</body>
</html>